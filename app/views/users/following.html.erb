<p id="notice"><%= notice %></p>

<h1>Followeing</h1>

<table>
  <thead>
    <tr>
      <th>Username</th>
      <th>Email</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.username %></td>
        <td><%= user.email %></td>
        <td><%= link_to 'visit user profile', user %></td> 
      </tr>
    <% end %>
  </tbody>
</table>
<%= link_to 'back', user_path(@user) %>   

